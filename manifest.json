{
    "name": "Propulsion",
    "version": "1.0.1",
    "description": "A motivational new tab page.",
    "chrome_url_overrides": {
        "newtab": "newpage.html"
    },
    "icons": {
        "16": "images/32x32.png",
        "48": "images/48x48.png",
        "128": "images/128x128.png"
    },
    "browser_action": {
        "default_icon": "images/128x128.png",
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker":"popup.js"
    },
    "content_security_policy": {
        "default-src" : "self",
        "object-src" : "self"
    },

    "manifest_version": 3
}