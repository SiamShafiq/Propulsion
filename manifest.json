{
    "name": "Propulsion",
    "version": "1.0.0",
    "description": "Motivational Theme!",
    "permissions": ["storage",
        "tabs",
        "notifications",
        "http://*/",
        "https://*/"
    ],
    "chrome_url_overrides":{
        "newtab":"newpage.html"
    },

    "icons": 
        {
            "16": "images/propulsion_16.png",
            "48": "images/propulsion_48.png",
            "128": "images/propulsion_128.png"

        },
    "browser_action":{
        "default_icon":"images/propulsion_128.png",
        "default_popup":"popup.html"
    },

    "background": {
        "scripts": ["popup.js"],
        "persistent": false
      },

      "content_security_policy": "script-src 'self' https://backend.deviantart.com; object-src 'self'",
      


    "manifest_version": 2
  }